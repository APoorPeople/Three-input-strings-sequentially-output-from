#include<iostream>
#include<string>
using namespace std;
void swap(string &a,string &b);
int main()
{
    string a,b,c;
    cin>>a>>b>>c;
    if(a>b)
      swap(a,b);
    if(b>c)
      swap(b,c);
      if(a>b)
        swap(a,b);
      cout<<a<<endl;
      cout<<b<<endl;
      cout<<c<<endl;
      return 0;
}
void swap(string &a,string &b)
{
 string c;
c=a;
a=b;
b=c;
}
